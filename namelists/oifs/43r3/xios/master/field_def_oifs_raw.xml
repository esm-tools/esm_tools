        <field id="pv" long_name="potential vorticity" standard_name="potential_vorticity" unit="K*m2*kg-1*s-1"/>
        <field id="crwc" long_name="precipitation rain water specific content" standard_name="precipitation_rain_water_specific_content" unit="kg*kg-1"/>
