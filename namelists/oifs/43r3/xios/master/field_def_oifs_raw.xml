        <field id="vite" long_name="vertically integrated total energy" standard_name="atmosphere_energy_content" unit="J m-2"/>
        <field id="sz" long_name="geopotential at surface" standard_name="surface_geopotential" unit="m2 s-2"/>
        <field id="sp" long_name="surface pressure" standard_name="surface_air_pressure" unit="Pa"/>
        <field id="tcw" long_name="total column water" standard_name="atmosphere_mass_content_of_water" unit="kg m-2"/>
        <field id="tcwv" long_name="total column water vapor" standard_name="atmosphere_mass_content_of_water_vapor" unit="kg m-2"/>
        <!--TODO: create single multi-layer field-->
        <field id="stl1" long_name="soil temperature layer 1" standard_name="soil_temperature_layer_1" unit="K"/>
        <field id="stl2" long_name="soil temperature layer 2" standard_name="soil_temperature_layer_2" unit="K"/>
